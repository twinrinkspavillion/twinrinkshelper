@page
@model DP.TwinRinksHelper.Web.Pages.ScheduleSyncSpec.CreateModel
@{
    ViewData["Title"] = "Create Automatic Comparison";
}
<div class="panel panel-default" style="margin-top:10px">
    <div class="panel-heading">Create Automatic Comparison</div>
    <div class="panel-body">
        <form method="post">
            <fieldset>
                <div class="form-group">
                    <label>TeamSnap Team:</label>
                    @Html.DropDownListFor(m => m.SelectedTeamSnapTeamId, Model.GetTeamSnapTeamsItems())
                </div>
                <div class="form-group">
                    <label>TwinRinks Team:</label>
                    @Html.DropDownListFor(m => m.SelectedTwinRinksTeam, Model.GetTwinRinksTeamsItems())
                </div>
                <div class="form-group">
                    <label>Expires:</label>
                    <input asp-for="SelectedExpiresDate" class="form-control" />
                </div>
                <button type="submit" class="btn btn-primary">Create</button>
            </fieldset>
        </form>
    </div>
</div>
